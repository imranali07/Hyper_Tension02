<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dell.simpleapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".Activities.RegistrationActivity" />
        <activity android:name=".Activities.MainActivity" />
        <activity android:name=".Activities.SearchActivity" />
        <activity android:name=".Activities.PatientMedicalHistoryActivity" />
        <activity android:name=".Activities.BloodPressure" />
        <activity android:name=".Activities.MedicineActivity" />
        <activity android:name=".Activities.NextAppointmentActivity"></activity>
    </application>

</manifest>